.banner{
height: 600rpx;
width: 100%;
}
.banner image{
width: 100%;
height: 100%;
z-index: 0;
}

.button{
  position: relative;
  height: 200rpx;
  top: -150rpx;
  text-align: center;

}


.button image{
  width: 300rpx;
  height: 300rpx;
}



.shop,.tongue{
  display: inline-block;
  height: 100rpx;
}

.card{
  height: 250rpx;
  width: 80%;
  margin: auto;
  border-radius: 40rpx;
  position: relative;
}

.card_text{
  height: 125rpx;
  line-height: 125rpx;
  padding-left: 20%;
  position: relative;
}

.card_icon{
  position: absolute;
    height: 100rpx;
  line-height: 100rpx;
  width: 50rpx;
  display: inline-block;
  left:-50rpx;
  padding-top: 25rpx;
}

.card_icon image{
      height: 100rpx;
  width: 100rpx;
}

.chart{
  margin-top: 50rpx;
  width: 80%;
  margin-left: auto;
  margin-right: auto;
  text-align: center;
  margin-bottom: 50rpx;
}
.chart_text{
  display: inline-block;
  width: 50%;
  text-align: center;
}



.click
{
 transform:scale(0.92);
  opacity: 0.5
}


.solar_radius view{
  width: 20rpx;
  height: 40rpx;
  border-radius: 999px;
  border-style: solid;
  border: 1px;
  border-color: #e8cc9c;
  border-width: 1px;
}
.solar_radius {
width: 40rpx;
height: 40rpx;
background-color: #e8cc9c;
text-align: center;
padding-top: 5rpx;
}
.solar_text{
  display: inline-block;
  padding-left: 50rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding-bottom: 20rpx;
  color: #e8cc9c;
  letter-spacing: 3px;
  font-size: 28rpx;
} 

.solar_image_title{
  font-size: 52rpx;
  color: #000;
  position: absolute;
  letter-spacing: 5px;
  left:60rpx;
  top: 80rpx;
}
.solar_image_eng{
  color: #96ccb4;
  font-size: 24rpx;
  position: absolute;
  left:60rpx;
  top: 150rpx;
}
.solar_image_date{
  color: #96ccb4;
  font-size: 24rpx;
  position: absolute;
  left:240rpx;
  top: 90rpx;
}
.solar_image_cns{
  width: 300rpx;
  font-size: 26rpx;
  color: #000;
  position: absolute;
  left:240rpx;
  top: 130rpx;
}

.solar_image{
  position: relative;
  width: 80%;
  height: 300rpx;
  margin: auto;
  background-repeat:no-repeat;
	background-size:100% 100%;
  -moz-background-size:100% 100%;
  background-image: url('data:image/png;base64,');
}


