.weather{
  margin-top: 50rpx;
}
.weather_text{
  padding-left: 50rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding-bottom: 20rpx;
  color: #e8cc9c;
  letter-spacing: 3px;
  font-size: 28rpx;
} 

.weather_container{
  text-align: center;
}
.weather_image{
display: inline-block;
margin-left: 20rpx;
position: relative;
}
.weather_image_text{
  position: absolute;
  top:70rpx;
  left:20rpx;
  font-size: 52rpx;
  color: #e8cc9c;
}
.weather_image image{
  width: 200rpx;
  height: 220rpx;

}


.tips{
  margin-top: 50rpx;
  text-align: center;
}
.tips_text{
  width:250rpx;
  position: absolute;
  left:45rpx;
  top:120rpx;
font-size: 24rpx;
text-align: left;
opacity: 0.8;
}
.tips_image{
  display: inline-block;
  position: relative;
}

.tips_image image{
  height: 300rpx;
  width: 340rpx;
}